{
    "name": "pic picker",
    "version": "1.0",
    "manifest_version": 2,
    "description": "a browser action with a popup dump",
    "content_scripts": [{
        "matches": ["http://*.renren.com/*"],
        "js": [
            "js/require.js",
            "js/jquery-1.8.2.js",
            "js/main.js"
        ],
        "run_at": "document_end"
    }],
    "permissions": [
        "http://*.renren.com/"
    ]
}

